{
  "Serilog": {
    "Using:1": "Serilog.Sinks.Debug",
    "Using:2": "Serilog.Sinks.File",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "App": "Debug"
      }
    },
    "WriteTo:1": { "Name": "Debug" },
    "WriteTo:2": {
      "Name": "File",
      "Args": {
        "path": "Logs/sample-.log",
        "rollingInterval": "Day"
      }
    }
